<Application
    x:Class="Float.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Startup="Application_Startup">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/FloatResources;component/Styles/ColorResource.xaml" />
                <ResourceDictionary Source="pack://application:,,,/FloatResources;component/Styles/IconStyle.xaml" />
                <ResourceDictionary Source="pack://application:,,,/FloatResources;component/Themes.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
